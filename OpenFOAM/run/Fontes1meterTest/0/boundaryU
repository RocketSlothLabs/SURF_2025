FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }

    inlet
    {
        type            fixedValue;
        value           uniform (0 0 -3050);
    }

    lateralWall
    {
        type            noSlip;
    }

    floorWall
    {
        type            noSlip;
    }

    symmetryX
    {
        type            symmetry;
    }

     symmetryY
    {
        type            symmetry;
    }
}


