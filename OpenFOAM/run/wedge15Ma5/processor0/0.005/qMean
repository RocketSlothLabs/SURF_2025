{
    flow
    {
        type            calculated;
        value           uniform 0;
    }
    obstacle
    {
        type            calculated;
        value           nonuniform List<scalar> 10(3048.615271 2630.697388 2364.835768 2084.483166 1983.760995 1722.704525 1629.770989 1572.993571 1498.804802 1430.376909);
    }
    frontAndBack
