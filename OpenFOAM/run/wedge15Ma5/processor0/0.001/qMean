{
    flow
    {
        type            calculated;
        value           uniform 0;
    }
    obstacle
    {
        type            calculated;
        value           nonuniform List<scalar> 10(3098.834787 2726.49924 2552.782466 2035.757533 1993.420527 1658.756036 1661.894517 1670.193567 1499.372208 1497.447005);
    }
    frontAndBack
