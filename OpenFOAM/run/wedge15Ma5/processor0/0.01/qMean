FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.01";
    object      qMean;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    flow
    {
        type            calculated;
        value           uniform 0;
    }
    obstacle
    {
        type            calculated;
        value           nonuniform List<scalar> 10(3097.427278 2640.236403 2333.276049 2098.17111 1923.67314 1733.804148 1669.765417 1576.840676 1468.574162 1409.134611);
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 0;
    }
}


