\*---------------------------------------------------------------------------*/

FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rhoM;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];  // Units: kg/m^3
internalField   uniform 1.66054e-4;  // ~mass of 1e20 particles * 1.66e-27 kg

boundaryField
{
    inlet
    {
        type zeroGradient;
    }
    outlet
    {
        type zeroGradient;
    }
    top
    {
        type zeroGradient;
    }
    bottom
    {
        type zeroGradient;
    }
    front
    {
        type zeroGradient;
    }
    back
    {
        type zeroGradient;
    }
}

// ************************************************************************* //
